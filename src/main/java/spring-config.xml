<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
       <context:annotation-config></context:annotation-config>
       <context:component-scan base-package="main.java.quoters"/>
       <bean class="main.java.quoters.DeprecationHandlerBeanFactoryPostProceccor"/>
       <bean class="main.java.quoters.PostProxyInvokerContextListener"/>
       <bean class="main.java.quoters.ProfilingHandlerBeanPostProcessor"/>
       <bean class="main.java.quoters.InjectRandomIntAnnotationBeanPostProcessor"/>
       <bean class="main.java.quoters.TerminatorQuoter" id ="terminatorQuoter">
              <property name="message" value="I'll be back"/>
       </bean>
</beans>